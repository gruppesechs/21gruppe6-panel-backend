{
  "name": "21gruppe6-panel-backend",
  "version": "1.0.0",
  "description": "Web panel backend for the Gruppe Sechs company of the 21 Jump Click GTA RP server.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmauNeko/21gruppe6-panel-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fmauNeko/21gruppe6-panel-backend/issues"
  },
  "homepage": "https://github.com/fmauNeko/21gruppe6-panel-backend#readme",
  "dependencies": {
    "@prisma/client": "^3.4.2",
    "apollo-server-core": "^3.5.0",
    "apollo-server-fastify": "^3.5.0",
    "class-validator": "^0.13.1",
    "fastify": "^3.24.0",
    "fastify-autoload": "^3.9.0",
    "fastify-cli": "^2.13.0",
    "fastify-jwt": "^3.2.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "graphql": "^15.7.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.13.6",
    "libsodium-wrappers": "^0.7.9",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/libsodium-wrappers": "^0.7.9",
    "@types/node": "^16.11.7",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "fastify-tsconfig": "^1.0.1",
    "husky": "^7.0.4",
    "prisma": "^3.4.2",
    "tap": "^15.1.0",
    "ts-node": "^10.4.0",
    "typegraphql-prisma": "^0.16.6",
    "typescript": "^4.4.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
